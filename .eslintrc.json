{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base",
        "airbnb-typescript/base"
    ],
    "plugins": [
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto",
            }
        ]
    }
}
